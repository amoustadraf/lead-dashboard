generator client {
  provider = "prisma-client-js"
  output   = "../src/app/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Lead {
  id            String    @id @default(uuid()) @map("ai_table_identifier") @db.Uuid
  row_number    Int?
  first_name    String?
  last_name     String?
  email         String?
  company       String?
  title         String?
  was_contacted Boolean?
  reply_date    DateTime? @db.Date
  created_at    DateTime  @default(now()) @map("created_at") @db.Timestamptz(6)
  email_subject String?
  email_body    String?   @db.Text
  email_sent    Boolean?  @default(false)

  @@map("ai_table_sheet1")
}
